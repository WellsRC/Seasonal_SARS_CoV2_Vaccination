clear;
clc;
parpool(32)

for ss=1:1000
    NS=10^4;

    DI=cell(NS,1);
    DH=cell(NS,1);
    CI=cell(NS,1);
    CH=cell(NS,1);
    

    [T_Run,P] = Parameter_Sample(NS);

    parfor jj=1:NS
        [~,~,Daily_Incidence,~,Daily_Hospital] = Run_ODE(T_Run,P{jj});
        DH{jj}=Daily_Hospital;
        DI{jj}=Daily_Incidence;
    end
    save(['Sample_Gen_' num2str(ss) '.mat'],'P','T_Run','DH','DI','CI','CH');
end
